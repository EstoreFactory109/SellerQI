{
    "version": 2,
    "functions": {
        "api/**": {
            "regions": [
                "sfo1",
                "iad1"
            ]
        }
    },
    "builds": [
        {
            "src": "api/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/index.js"
        }
    ],
    "installCommand": "npm ci",
    "buildCommand": "echo 'Build complete - pino removed'"
}