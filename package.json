{
  "name": "server",
  "version": "1.0.0",
  "description": "A scalable production ready seller central analysing application ",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "dev": "nodemon server/index.js",
    "start": "node server/index.js",
    "worker": "node server/Services/BackgroundJobs/worker.js",
    "integration-worker": "node server/Services/BackgroundJobs/integrationWorker.js",
    "alerts:test-user": "node server/scripts/runAlertsWorkerForUser.js",
    "test": "npm run test:server && npm run test:client",
    "test:server": "jest --config server/jest.config.js",
    "test:server:watch": "jest --config server/jest.config.js --watch",
    "test:server:coverage": "jest --config server/jest.config.js --coverage",
    "test:client": "cd client && npm test",
    "prepare": "husky"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-sts": "^3.758.0",
    "@react-email/components": "^0.0.41",
    "amazon-sp-api": "^1.1.5",
    "aws4": "^1.13.2",
    "axios": "^1.8.3",
    "axios-retry": "^4.5.0",
    "bcryptjs": "^3.0.2",
    "chalk": "^4.1.2",
    "cloudinary": "^2.6.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csv-parse": "^6.1.0",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.2.1",
    "google-auth-library": "^10.1.0",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
